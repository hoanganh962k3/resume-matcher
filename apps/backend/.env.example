# Session Configuration
SESSION_SECRET_KEY="your-secret-key-change-in-production"

# Database Configuration
SYNC_DATABASE_URL="sqlite:///./app.db"
ASYNC_DATABASE_URL="sqlite+aiosqlite:///./app.db"

# Python Configuration
PYTHONDONTWRITEBYTECODE=1

# JWT Authentication (Optional - with defaults)
# JWT_SECRET_KEY="your-jwt-secret-key-change-in-production"
# JWT_ALGORITHM="HS256"
# JWT_ACCESS_TOKEN_EXPIRE_MINUTES=10080

# LLM Provider Configuration
# Supported providers: openai, anthropic, azure, etc.
LLM_PROVIDER="openai"
LLM_API_KEY="your-openai-api-key-here"
LLM_BASE_URL=""
LL_MODEL="gpt-4o-mini"

# Embedding Provider Configuration
EMBEDDING_PROVIDER="openai"
EMBEDDING_API_KEY="your-openai-api-key-here"
EMBEDDING_BASE_URL=""
EMBEDDING_MODEL="text-embedding-3-small"

# CORS Configuration (Optional - with defaults)
# ALLOWED_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

